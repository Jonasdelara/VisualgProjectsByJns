Algoritmo "notas"

Var

  nota1, nota2, media :real

  
Inicio

 escreva ("Escreava o valor da 1° nota: ")
  leia (nota1)

  escreva ("Escreva o valor da 2° nota: ")
  leia (nota2)
  
    media <- nota1 + nota2
    escreval("nota final:", media:5:1)
    
    se media < 60 entao
    escreva ("aluno reprovado")
        fimse


Fimalgoritmo
