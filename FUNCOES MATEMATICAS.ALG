Algoritmo "funcoes matematicas"

Var
x,y,z,a,b,c,d :real

Inicio
 x <- 2.0
 y <- 3.0
 z <- 7.0
 
 a <- exp (x,y)
 b <- raizQ (y)
 c <- pi
 d <- abs (z)
 
 escreval ("valor da expoenciação:",  (a))
 escreval ("valor da raiz quadrada:", (b))
 escreval ("valor de PI:", (c))
 escreval ("valor absoluto",(d))

Fimalgoritmo